{
    "name": "chroma",
    "displayName": "chroma",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "enableProposedApi": true,
    "main": "./dist/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "test-compile": "tsc -p ./",
        "test-watch": "tsc -watch -p ./",
        "pretest": "npm run test-compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/copy-webpack-plugin": "^8.0.0",
        "@types/eslint": "^7.2.11",
        "@types/file-loader": "^5.0.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/node-fetch": "^2.5.10",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "clean-webpack-plugin": "^4.0.0-alpha.0",
        "copy-webpack-plugin": "^9.0.0",
        "eslint": "^7.19.0",
        "file-loader": "^6.2.0",
        "glob": "^7.1.6",
        "mocha": "^8.2.1",
        "node-loader": "^2.0.0",
        "ts-loader": "^8.0.14",
        "typescript": "^4.1.3",
        "vscode-test": "^1.5.0",
        "webpack": "^5.19.0",
        "webpack-cli": "^4.4.0"
    },
    "dependencies": {
        "iohook": "file:./iohook",
        "jsonc": "^2.0.0",
        "mobx": "^6.3.2",
        "node-fetch": "^2.6.1",
        "pascal-case": "^3.1.2"
    },
    "iohook": {
        "targets": [
            "node-88",
            "electron-87"
        ],
        "platforms": [
            "win32",
            "darwin",
            "linux"
        ],
        "arches": [
            "x64",
            "ia32"
        ]
    }
}
